    var duration = 550;\n                jQuery(window).on(\"scroll\", function () {\n                    if (jQuery(this).scrollTop() > offset) {\n                        jQuery(\".progress-wrap\").addClass(\n                            \"active-progress\"\n                        );\n                    } else {\n                        jQuery(\".progress-wrap\").removeClass(\n                            \"active-progress\"\n                        );\n                    }\n                });\n                jQuery(\".progress-wrap\").on(\"click\", function (event) {\n                    event.preventDefault();\n                    jQuery(\"html, body\").animate(\n                        {\n                            scrollTop: 0,\n                        },\n                        duration\n                    );\n                    return false;\n                });\n            });\n        })(jQuery);\n    },\n};\n</script>\n\n<style scoped>\n.progress-wrap {\n    position: fixed;\n    right: 50px;\n    bottom: 50px;\n    height: 46px;\n    width: 46px;\n    cursor: pointer;\n    display: block;\n    border-radius: 50px;\n    box-shadow: inset 0 0 0 2px rgba(0, 0, 0, 0.2);\n    z-index: 10000;\n    opacity: 0;\n    visibility: hidden;\n    transform: translateY(15px);\n    -webkit-transition: all 200ms linear;\n    transition: all 200ms linear;\n    background-color: #198754;\n}\n\n.progress-wrap.active-progress {\n    opacity: 1;\n    visibility: visible;\n    transform: translateY(0);\n}\n\n.progress-wrap::after {\n    position: absolute;\n    content: \"↑\";\n    text-align: center;\n    line-height: 46px;\n    font-size: 24px;\n    color: #ffc107;\n    left: 0;\n    top: 0;\n    height: 46px;\n    width: 46px;\n    cursor: pointer;\n    display: block;\n    z-index: 1;\n    -webkit-transition: all 200ms linear;\n    transition: all 200ms linear;\n}\n\n.progress-wrap:hover::after {\n    opacity: 0;\n}\n\n.progress-wrap::before {\n    position: absolute;\n    content: \"↑\";\n    text-align: center;\n    line-height: 46px;\n    font-size: 24px;\n    opacity: 0;\n    background-image: linear-gradient(298deg, #ffffff, #ffffff);\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n    left: 0;\n    top: 0;\n    height: 46px;\n    width: 46px;\n    cursor: pointer;\n    display: block;\n    z-index: 2;\n    -webkit-transition: all 200ms linear;\n    transition: all 200ms linear;\n}\n\n.progress-wrap:hover::before {\n    opacity: 1;\n}\n\n.progress-wrap svg path {\n    fill: none;\n}\n\n.progress-wrap svg.progress-circle path {\n    stroke: #ffc107;\n    stroke-width: 5;\n    box-sizing: border-box;\n    -webkit-transition: all 200ms linear;\n    transition: all 200ms linear;\n}\n</style>"],"mappings":"AAUA,OAAOA,MAAK,MAAO,QAAQ;AAE3B,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,OAAOA,CAAA,EAAG;IACN,CAAC,UAAUC,CAAC,EAAE;MACV,YAAY;;MACZA,CAAC,CAACC,QAAQ,CAAC,CAACC,KAAK,CAAC,YAAY;QAC1B,YAAY;;QAEZ,IAAIC,YAAW,GAAIF,QAAQ,CAACG,aAAa,CACrC,qBACJ,CAAC;QACD,IAAIC,UAAS,GAAIF,YAAY,CAACG,cAAc,CAAC,CAAC;QAC9CH,YAAY,CAACI,KAAK,CAACC,UAAS,GACxBL,YAAY,CAACI,KAAK,CAACE,gBAAe,GAAI,MAAM;QAChDN,YAAY,CAACI,KAAK,CAACG,eAAc,GAC7BL,UAAS,GAAI,GAAE,GAAIA,UAAU;QACjCF,YAAY,CAACI,KAAK,CAACI,gBAAe,GAAIN,UAAU;QAChDF,YAAY,CAACS,qBAAqB,CAAC,CAAC;QACpCT,YAAY,CAACI,KAAK,CAACC,UAAS,GACxBL,YAAY,CAACI,KAAK,CAACE,gBAAe,GAClC,+BAA+B;QACnC,IAAII,cAAa,GAAI,SAAAA,CAAA,EAAY;UAC7B,IAAIC,MAAK,GAAId,CAAC,CAACe,MAAM,CAAC,CAACC,SAAS,CAAC,CAAC;UAClC,IAAIC,MAAK,GAAIjB,CAAC,CAACC,QAAQ,CAAC,CAACgB,MAAM,CAAC,IAAIjB,CAAC,CAACe,MAAM,CAAC,CAACE,MAAM,CAAC,CAAC;UACtD,IAAIC,QAAO,GACPb,UAAS,GAAKS,MAAK,GAAIT,UAAU,GAAIY,MAAM;UAC/Cd,YAAY,CAACI,KAAK,CAACI,gBAAe,GAAIO,QAAQ;QAClD,CAAC;QACDL,cAAc,CAAC,CAAC;QAChBb,CAAC,CAACe,MAAM,CAAC,CAACD,MAAM,CAACD,cAAc,CAAC;QAChC,IAAIM,MAAK,GAAI,EAAE;QACf,IAAIC,QAAO,GAAI,GAAG;QAClBvB,MAAM,CAACkB,MAAM,CAAC,CAACM,EAAE,CAAC,QAAQ,EAAE,YAAY;UACpC,IAAIxB,MAAM,CAAC,IAAI,CAAC,CAACmB,SAAS,CAAC,IAAIG,MAAM,EAAE;YACnCtB,MAAM,CAAC,gBAAgB,CAAC,CAAC